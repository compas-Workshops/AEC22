                    <item name="CodeInput" type_name="gh_string" type_code="10">import math

from compas.geometry import Box
from compas.geometry import Plane
from compas.geometry import Frame
from compas.geometry import Vector
from compas.geometry import Translation
from compas.geometry import Polygon
from compas.geometry import Projection
from compas.datastructures import Mesh
from separation_axis_theorem import separating_axis_theorem

projection = Projection.from_plane(Plane.worldXY())

for part in assembly.parts():
    # Store XY projection
    projected_mesh = Mesh.from_shape(part.geometry.transformed(projection))
    part.attributes['projection'] = Polygon(projected_mesh.face_coordinates(0))

    # Calculate place frame
    part.attributes['place_frame'] = part.frame.transformed(Translation.from_vector([0, 0, part.attributes['zsize']]))

# Connect assembly graph
last_course_parts = None
for c in range(assembly.attributes['courses']):
    course_parts = list(assembly.graph.nodes_where_predicate(lambda key, attr: attr['part'].attributes['course'] == c))

    if last_course_parts:
        for key in course_parts:
            part1 = assembly.find_by_key(key)
            polygon1 = part1.attributes['projection']

            for p2_key in last_course_parts:
                part2 = assembly.find_by_key(p2_key)
                polygon2 = part2.attributes['projection']
                are_connected = separating_axis_theorem(polygon1.points, polygon2.points)
                if are_connected:
                    assembly.add_connection(part2, part1)

    last_course_parts = course_parts
